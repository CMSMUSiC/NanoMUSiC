import "$PXLANA/ConfigFiles/Objects/jet.cff" with "Jet" as "FatJet"

FatJet.use = 0
FatJet.UsePUPPI = 0
FatJet.PUPPI.SDMassCorrection = "$PXLANA/ConfigFiles/ConfigInputs/puppiCorr.root"
FatJet.Type.Gen = ""
FatJet.Type.Rec = "AK8"
FatJet.pt.min = 200

FatJet.ID.Tag = 0

FatJet.WJets.use = 0
FatJet.WJets.Algo = "prunedMass"
FatJet.WJets.tau1 = "tau1"
FatJet.WJets.tau2 = "tau2"
FatJet.WJets.mass.min = 65
FatJet.WJets.mass.max = 95
FatJet.WJets.tau.max = 0.4

FatJet.JSONFile = "$PXLANALYZER_BASE/ConfigFiles/ConfigInputs/jsonPOG/JME/2017_UL/fatJet_jerc.json"
# needed untill 2017 SFs are provided
FatJet.JSONFile.2018 = "$PXLANALYZER_BASE/ConfigFiles/ConfigInputs/jsonPOG/JME/2018_UL/fatJet_jerc.json"

